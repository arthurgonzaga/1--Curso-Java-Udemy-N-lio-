package entities;

public class Charmander {
	public double vida = 5000;
	public double ataque = 44;
	public double defesa = 18;
	public double agilidade = 15;

	
	
	
	public double charmanderEmber() {
		double danoBaseEmber = 30;
		return danoBaseEmber + ataque + agilidade - this.defesaMewtwo;
		
	}
}



